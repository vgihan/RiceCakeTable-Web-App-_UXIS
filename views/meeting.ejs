					<textarea id ="msg_box" onkeyup="enterkey()" placeholder="메세지를 입력하세요"></textarea>
					<div class="btn_file">
						<a class="btn" input type="button" onclick="send_msg()">전송</a>
						<a href="" class="file"><img src="img/icon_file.png"/></a>
					</div>
				</div>
			</div>
			<!-- chatbox : e -->

			<div class="cont">
				<div class="inner">
